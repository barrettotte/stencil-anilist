{
  "name": "anilist-stencil",
  "version": "0.0.1",
  "description": "Stencil.js component to display a public Anilist user.",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/anilist-stencil/anilist-stencil.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "stencil build --dev --watch --serve",
    "start-mock": "nodemon test/mock-server.js --watch",
    "build-dev": "stencil build --dev --docs",
    "build-prod": "stencil build --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.9.0",
    "d": "^1.0.1"
  },
  "devDependencies": {
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jest": "^27.2.5",
    "jest-cli": "^27.2.5",
    "nodemon": "^2.0.20",
    "puppeteer": "^19.4.0"
  },
  "license": "MIT"
}
